{
    "name": "lwc-mobile-samples",
    "private": true,
    "version": "0.0.1",
    "description": "LWC Mobile Samples",
    "repository": {
        "type": "git",
        "url": "https://github.com/forcedotcom/LWC-Mobile-Samples"
    },
    "license": "MIT",
    "scripts": {
        "format": "prettier --check '**/*.{js,ts,json,md}'",
